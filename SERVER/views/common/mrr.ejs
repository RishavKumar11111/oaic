<div id="MRR" class="focusprintpage">
    <style>
      .underline {
        border-bottom: 1px solid black;
        padding-left: 15px;
        padding-right: 15px;
      }
    </style>
    <link href="../../new/css/sb-admin.css" rel="stylesheet">
    <div class="card">
      <div class="card-body">
        <div class="card-text">





          <div class="container">




            
        <style>
          .heading {
            padding: 1%;
            border-top: 1px solid;
            border-bottom: 1px solid;
          }
          .col-6, .col-4{
            padding: 0;
          }
          .main-page {
            border: 1px solid;
          }
          .body {
            padding: 1%;
          }
          .row {
            margin-left: 0;
            margin-right: 0;
            margin-bottom: 0;
          }
          h6, h3 {
            font-weight: bold;
          }
          hr {
            border-top: 1px solid;
          }
          .table th, .table td {
            padding: 0.35rem;
          }
        </style>



        
        <div class="main-page">


            
          <div class="row">
            <div class="col-8" style="border-right: 1px solid;">
                <div class="row">
                  <div class="col-3 body">
                    <img src="/images/oaic-new-logo.png" alt="" style="width: 100%;">
                  </div>
                  <div class="col-9 body">
                    <h5 style="text-align: center;font-weight: bold;white-space: nowrap;margin-left: -4%;">THE ODISHA AGRO INDUSTRIES CORPORATION LTD</h5>
                    <p style="text-align: center;">(A Government of Odisha Undertaking)<br>
                      Regd. Office: 95, Satyanagar, Bhubaneswar-751007  <br> www.orissaagro.com <br>
                      GSTIN: 21AABCT5432F2ZT</p>
                      <h6 style="text-align: center;">OFFICE OF THE DISTRICT MANAGER, {{ DMDetails.dist_name }}</h6>
                      <p style="text-align: center;">{{ DMDetails.dm_address }}
                        <br> Email-ID: {{ DMDetails.EmailID }}, Contact Number: {{ DMDetails.dm_mobile_no }} / {{ AccountantDetails.acc_mobile_no }}
                        </p>
                  </div>
                </div>
            </div>
            <div class="col-4">
              <div style="border-bottom: 1px solid;height: 80px;">
                <h3 style="text-align: center;">MATERIAL RECEIPT REPORT</h3>
              </div>
              <div>
                <br>
                <h5 style="text-align: center;">MRR No: {{ mrr_id }} <br>
                  <!-- MRR number format - SLR/PO/2021-22/001 -->
                  Date: {{ received_date | date: 'd-MM-y' }}
              </h5>
              </div>
            </div>
          </div>

          <div class="row">
            <div class="col-6">
              <div class="heading">
                <h5 style="font-weight: bold;">VENDOR DETAILS</h5>
              </div>
              <div class="body">
                {{ dl.LegalBussinessName }} <br>
                {{ dl.Address }} <br>
                E-mail: {{ dl.EmailID }}<br>
                Phone No.: {{ dl.ContactNumber }}<br>
                GSTIN: {{ dl.GSTN }}<br>

              </div>
            </div>
            <div class="col-6" style="border-left: 1px solid;">
              <div style="border-top: 1px solid;padding: 1%;">
                <h6>Supplier Invoice No.: {{ invoice.invoice_no }} </h6>
                <h6>Supplier Invoice Date: {{ invoice.bill_date | date: 'dd-MM-yyyy' }} </h6>
                <hr>
                <h6>PO Number  : {{ invoice.indent_no }} </h6>
                <h6>PO Date: {{ invoice.InsertedDate | date: 'dd-MM-yyyy' }} </h6>
              </div>
            </div>
          </div>



          

          <div class="row">
              <div class="col-lg-12 table-responsive">
                    
                  <table class="table table-bordered">
                    <caption></caption>
                    <thead>
                      <tr>
                        <th scope="col">SL</th>
                        <th scope="col">Description of Goods</th>
                        <th scope="col">Quantity as per PO</th>
                        <th scope="col">Actual Quantity Received</th>
                        <th scope="col">Unit</th>
                        <th scope="col">Rate per Unit (&#8377;)</th>
                        <th scope="col">Taxable Value (&#8377;)</th>
                        <th scope="col">GST Rate</th>
                        <th scope="col">CGST (&#8377;)</th>
                        <th scope="col">SGST (&#8377;)</th>
                        <th scope="col">Invoice Value (&#8377;)</th>
                      </tr>
                    </thead>
                    <tbody>
                      <tr ng-repeat="x in printData">
                        <td>{{ $index + 1 }}</td>
                        <td style="text-align: left;">
                          Product Category : {{ x.Implement}} <br>
                          Manufacturer : {{ x.Make}} <br>
                          Model/Item : {{ x.Model }} <br>
                          <div ng-show="false">
                              Engine No. {{ x.engine_no }} <br>
                              Chasis No.   {{ x.chassic_no }}
                          </div>
                        </td>
                        <td>{{ x.ItemQuantity | number }}</td>
                        <td>{{ x.ItemQuantity }}</td>
                        <td>{{ x.UnitOfMeasurement }}</td>
                        <td>{{ x.PurchaseTaxableValue | number :'2' }}</td>
                        <td>{{ x.TotalTaxableValue | number :'2' }}</td>
                        <td>{{ x.TaxRate }}%</td>
                        <td>{{ x.TotalCGST | number :'2' }}</td>
                        <td>{{ x.TotalSGST | number :'2' }}</td>
                        <td>{{ x.TotalInvoiceValue | number :'2' }}</td>
                      </tr>
                      <tr>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td><strong>Total(&#8377;)</strong> </td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td></td>
                        <td>{{ totalPriceInTable | number: '2'}}</td>
                        
                      </tr>
                    </tbody>
                  </table>
              </div>
          </div>

          
          <div class="row body">
            <div class="col-12">
              <h6 style="text-align: center;">For Odisha Agro Industries Corporation Ltd.</h6>
            </div>
            <div class="col-4" style="text-align: left;margin-top: 60px;">
              <h5>Store Keeper</h5>
            </div>
            <div class="col-4" style="text-align: center;margin-top: 60px;">
              <h5>Accountant</h5>
            </div>
            <div class="col-4" style="text-align: right;margin-top: 60px;">
              <h5>Project/District/Factory Manager</h5>
            </div>
          </div>
          

        </div>



          </div>





        </div>
      </div>
    </div>
  </div>