<!-- ========================== ENTRY ITEM DETAIL START ==========================-->

<div >
  <form name="CPForm">
    <div class="mybox">
      <div class="mybox-header">
        <span>
          <strong>
            <label>Change Password</label>
          </strong>
        </span>
      </div>
      <div class="middle s-form-card">
            <div class="small-form">
                <div class="row">
                    <label class="col-md-4 col-lg-4">Old password<span class="asterisk-mark">*</span>:</label>
                    <input class="col-md-8 col-lg-8" type="password" class="form-control" ng-model="oldPassword" placeholder="Enter your old password" required>
                </div>
                <div class="row">
                        <label class="col-md-4 col-lg-4">New password<span class="asterisk-mark">*</span>:</label>
                        <input class="col-md-8 col-lg-8" type="password" class="form-control" ng-model="newPassword" placeholder="Enter new password to change" required>
                </div>
                <div class="row">
                        <label class="col-md-4 col-lg-4">Confirm password<span class="asterisk-mark">*</span>:</label>
                        <input class="col-md-8 col-lg-8" type="password" class="form-control" ng-model="confirmPassword" placeholder="Re-enter new password for confirmation" required>
                </div>
                <div>
                    {{ message }}
                </div>
                <div class="row">
                    <div class="col-md-4 col-lg-4"></div>
                    <div class="col-md-8 col-lg-8">
                        <button class="btn btn-secondary"><i class="fas fa-sync" aria-hidden="true"></i> Reset Form</button>
                        <button class="btn btn-info" ng-click="changePassword()"><i class="fas fa-paper-plane" aria-hidden="true"></i> Submit </button>
                    </div>
                </div>
            </div>
      </div>
    </div>
  </form>
</div>

<!-- ========================== ENTRY ITEM DETAIL ENDS ==========================-->

<script src="/controller/changePassword.js"></script>















